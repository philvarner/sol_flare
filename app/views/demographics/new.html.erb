<h1>New Demographic</h1>


<%= form_for :demographic, url: demographics_path do |f| %>
    <p>
      <%= f.label :school_id %><br>
      <%= f.text_field :school_id %>
    </p>

    <p>
      <%= f.label :division_id %><br>
      <%= f.text_area :division_id %>
    </p>

    <p>
      <%= f.submit %>
    </p>
<% end %>